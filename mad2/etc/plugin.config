checksum:
  module: mad2.plugin.checksum
samplesheet:
  module: mad2.plugin.samplesheet
onthefly:
  module: mad2.plugin.onthefly
set:
  module: mad2.plugin.madset
find:
  module: mad2.plugin.find
select:
  module: mad2.plugin.select
table:
  module: mad2.plugin.table
print:
  module: mad2.plugin.print
backup:
  module: mad2.plugin.backup
cpmv:
  module: mad2.plugin.cpmv
projector:
  module: mad2.plugin.projector
x:
  enabled: true
  module: mad2.plugin.x
  default_executor: simple
  annotate:
    createdBy: "$(id -un)@$(hostname)"
    createdHost: "$(hostname)"
    createdOn: '$(date  +"%Y-%m-%dT%H:%M:%S")'
    derivedFrom: "{{ fullpath }}"
    derivedFromSha1: '{{ hash.sha1 }}'
neo4j:
  enabled: false
  autostore: false
  module: mad2.plugin.neo
  uri: 'http://localhost:7474/db/data/'
root:
  enabled: true
  module: mad2.plugin.root
lcbhacks:
  enabled: true
  module: mad2.plugin.lcbhacks
category:
  module: mad2.plugin.category
mongo:
  module: mad2.plugin.mongo
  enabled: false
  indici:
    - project
    - userid
    - sha1
    - username
    - host
    - filesize
    - dirname
    - category
    - extension
    - basename
